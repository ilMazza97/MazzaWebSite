        <div class="row">
            <div class="col-lg-12">
                <h2 class="title-1 m-b-25">Earnings By Items</h2>
                <div class="table-responsive table--no-card m-b-40">
                    <table class="table table-borderless table-striped table-earning">
                        <thead>
                            <tr>
                                <th>Package Name</th>
                                <th>Referent Name</th>
                                <th>Referent LastName</th>
                                <th>Package Total</th>
                                <th>Package Percentage</th>
                                <th>Deposit Percentage</th>
                                <th>Package Percentage</th>
                                <th>Profit</th>
                                <th>Accumulate Commission</th>
                                <th>Destinate Commission</th>
                                <th>Commission %</th>
                                <th>Next Payout</th>
                               
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var investment in Model)
                            {
                                <tr>
                                    <td>@investment.PackageName</td>
                                    <td>@investment.ReferentName</td>
                                    <td>@investment.ReferentLastName</td>
                                    <td>@investment.PackageTotal</td>
                                    <td>@investment.PackagePercentage</td>
                                    <td>@investment.DepositPercentage</td>
                                    <td>@investment.PackagePercentage</td>
                                    <td>@investment.Profit</td>
                                    <td>@investment.AccumulateCommission</td>
                                    <td>@investment.DestinateCommission</td>
                                    <td>@investment.Commission</td>
                                    <td>@investment.NextPayout.ToShortDateString()</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

